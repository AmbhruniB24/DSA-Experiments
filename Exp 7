#include <stdio.h>
#include <stdlib.h>

struct Node {
    int data;
    struct Node *next;
};

struct Node *head = NULL;

void insert(int x) {
    struct Node *newNode = malloc(sizeof(struct Node)), *temp;
    newNode->data = x;
    if (head == NULL) {
        head = newNode;
        newNode->next = head;
    } else {
        temp = head;
        while (temp->next != head) temp = temp->next;
        temp->next = newNode;
        newNode->next = head;
    }
}

void display() {
    if (head == NULL) { printf("List empty\n"); return; }
    struct Node *temp = head;
    do {
        printf("%d ", temp->data);
        temp = temp->next;
    } while (temp != head);
    printf("\n");
}

int main() {
    int n, val;
    printf("Enter number of nodes: ");
    scanf("%d", &n);
    for (int i = 0; i < n; i++) {
        printf("Enter data: ");
        scanf("%d", &val);
        insert(val);
    }
    printf("Circular Linked List: ");
    display();
    return 0;
}
